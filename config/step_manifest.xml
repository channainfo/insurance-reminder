<?xml version="1.0" encoding="UTF-8" ?>
<verboice-service>
  <name>Insurance Reminder</name>
  <global-settings>
    <variable name="service_domain" display-name="Service Domain" type="string"/>
  </global-settings>
  <steps>
    <step name="notify_call_started"
      display-name="Notify Call Started"
      icon="medicalkit"
      type="callback"
      callback-url="http://{service_domain}/calls/notify_call_started">
      <settings/>
      <response type="none"/>
    </step>
  </steps>
  <steps>
    <step name="notify_call_finished"
      display-name="Notify Call Finished"
      icon="medicalkit"
      type="callback"
      callback-url="http://{service_domain}/calls/notify_call_finished">
      <settings/>
      <response type="none"/>
    </step>
  </steps>
</verboice-service>